datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Category {
  id    Int     @id @default(autoincrement())
  title String 
  description  String
  color String
  notes Notes[]
}


model Notes {
  id    Int     @id @default(autoincrement())
  text String
  categoryId Int 
  category Category @relation(fields: [categoryId], references: [id])

}